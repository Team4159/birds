{
  "name": "birds",
  "version": "0.1.0",
  "description": "CardinalBotics Beginning and Intermediate Role Development System.",
  "main": "app.js",
  "readme": "README.md",
  "scripts": {
    "start": "NODE_ENV=PROD node start.js",
    "start:dev": "NODE_ENV=DEV JWT_SECRET='correcthorsebatterystaple' MONGODB_URI='mongodb://localhost/passport' nodemon start.js",
    "serve": "URL_PREFIX='http://localhost:8000' webpack-dev-server --progress --config birds/webpack-dev.config.js --inline --hot",
    "build:dev": "webpack --progress --config birds/webpack-dev.config.js",
    "build:prod": "webpack --progress --config birds/webpack-prod.config.js",
    "test": "NODE_ENV=TEST npm run test:fe && npm run test:api",
    "test:api": "NODE_ENV=TEST JWT_SECRET=TEST MONGODB_URI='mongodb://localhost/passport' mocha birds/test/*.js",
    "test:api-watch": "NODE_ENV=TEST JWT_SECRET=TEST MONGODB_URI='mongodb://localhost/passport' mocha birds/test/*.js --watch",
    "test:fe": "NODE_ENV=TEST URL_PREFIX='' find ./birds -name '*.test.js' | xargs mocha --opts unit.mocha.opts",
    "test:fe-watch": "NODE_ENV=TEST URL_PREFIX='' find ./birds -name '*.test.js' | xargs mocha --opts unit.mocha.opts --watch --reporter min",
    "postinstall": "npm run build:prod",
    "clean": "rm -rf node_modules & rm -rf birds/public & rm -rf birds/static",
    "lint": "find ./birds -name '*.js' -not -path '*/static/*' -not -path '*/public/*' | xargs eslint",
    "lint:fix": "find ./birds -name '*.js' -not -path '*/static/*' -not -path '*/public/*' | xargs eslint --fix",
    "build:docs": "apidoc -i birds/api -o docs",
    "cover": "NODE_ENV=TEST JWT_SECRET='meh' MONGODB_URI='mongodb://localhost/passport' istanbul cover _mocha ./birds/**/*.test.js ./birds/test/*.js -- --opts unit.mocha.opts -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team4159/BIRDS.git"
  },
  "author": "Elias Lawson-Fox <eliaslfox@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Team4159/BIRDS/issues"
  },
  "homepage": "https://github.com/Team4159/BIRDS#readme",
