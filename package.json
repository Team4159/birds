{
  "name": "birds",
  "version": "0.1.0",
  "description": "CardinalBotics Beginning and Intermediate Role Development System.",
  "main": "app.js",
  "readme": "README.md",
  "scripts": {
    "start": "node start.js",
    "start:dev": "JWT_SECRET='correcthorsebatterystaple' MONGODB_URI='mongodb://localhost/passport' nodemon start.js",
    "serve": "URL_PREFIX='http://localhost:8000' webpack-dev-server --progress --config birds/webpack-dev.config.js --inline --hot",
    "build:dev": "webpack --progress --config birds/webpack-dev.config.js",
    "build:prod": "webpack --progress --config birds/webpack-prod.config.js",
    "test": "NODE_ENV=TEST mocha birds/test/*.js",
    "test:unit": "find ./birds -name '*.test.js' | xargs mocha --opts unit.mocha.opts",
    "test:unit-watch": "find ./birds -name '*.test.js' | xargs mocha --opts unit.mocha.opts --watch --reporter min",
    "postinstall": "npm run build:prod",
    "clean": "rm -rf node_modules & rm -rf birds/public & rm -rf birds/static",
    "lint": "find ./birds -name '*.js' -not -path '*/static/*' -not -path '*/public/*' | xargs eslint",
    "lint:fix": "find ./birds -name '*.js' -not -path '*/static/*' -not -path '*/public/*' | xargs eslint --fix",
    "build:docs": "apidoc -i birds/api -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team4159/BIRDS.git"
  },
  "author": "Elias Lawson-Fox <eliaslfox@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Team4159/BIRDS/issues"
  },
  "homepage": "https://github.com/Team4159/BIRDS#readme",
  "dependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-alpha.1",
    "cors": "2.8.1",
    "eslint-config-standard": "6.0.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-standard": "2.0.0",
    "express": "4.14.0",
    "express-jwt": "5.0.0",
    "express-session": "1.14.1",
    "html-webpack-plugin": "2.22.0",
    "jsonwebtoken": "7.1.9",
    "material-ui": "0.15.4",
    "moment": "2.15.0",
    "mongoose": "4.6.0",
    "morgan": "1.7.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "4.0.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
